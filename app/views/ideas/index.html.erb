<div class="container-fluid">
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-10">
      <h1>IdeaBox</h1>
      <input type="text" class="text-input prepopulate" id="filter" rel="Search" name="search" value="" />
    </div>
  </div>
  <div class="row">
    <div class="col-md-2"></div>
    <div class="col-md-4">
      <h3>Latest Ideas</h3>
      <div id="latest-ideas">
      </div>
    </div>
    <div class="col-md-4">
      <div class="form-group">
        <input class="form-control prepopulate" type="text" id="idea-title" rel="Idea Title">
        <textarea class="form-control prepopulate" type="text" id="idea-body" rel="Idea Body"></textarea>
      </div>
      <input class="btn btn-default pull-right" id="create-idea" type="button" name="submit" value="Create Idea">
    </div>
    <div class="col-md-2"></div>
  </div>
</div>
